<?xml version="1.0"?>

<PropertyList>
	<path>Empressarial.ac</path>
	
	<!-- Transposición de texturas nocturnas -->
	
<animation>
	<type>textranslate</type>
	<object-name>Empresarial_estuco</object-name>
	<object-name>Empresarial_ventanas</object-name>
	<property>/sim/time/sun-angle-rad</property>
		<step> 1.57 </step>
		<factor>0.318471338</factor>
	<center>
		<x-m> 0 </x-m>
		<y-m> 0 </y-m>
		<z-m> 0 </z-m>
	</center>
	<axis>
		<x> -1 </x>
		<y>  0 </y>
		<z>  0 </z>
	</axis>
</animation>
	
</PropertyList>